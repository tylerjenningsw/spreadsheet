{
  "name": "spreadsheet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "pnpm run build && echo 'Now drag the dist folder to vercel.com or run: npx vercel ./dist'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,css}\""
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.8"
  }
}
